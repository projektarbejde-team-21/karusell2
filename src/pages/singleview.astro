export async function getStaticPaths() {
  const url = "..."
  const options = {
    headers: {
      apikey: "..."
    }
  }
  
  const artists = await fetch(url, options).then(res=>res.json())
  
  return artists.map(artist => ({
      params: { artist: artist.slug }, // [artist]=artist.slug
      props: { artist } // send al data med som props
    }))
}
